

income <- relig_income

pivot_longer(income, cols = -1, names_to= "Income", values_to= "count")

billboard
billboard %>%
  pivot_longer(
    cols = starts_with("wk"),
    names_to = "week",
    names_prefix = "wk",
    values_to = "rank",
    values_drop_na = TRUE
  )

example_gymnastics_2 %>% 
  pivot_longer(
    cols = !country,
    names_to = c("event", "year"),
    names_sep = "_",
    values_to = "score"
  )
